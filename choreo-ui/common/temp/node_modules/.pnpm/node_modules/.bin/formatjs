#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rasikamaduranga/WS02/OpenChoreo/openchoreo/choreo-ui/common/temp/node_modules/.pnpm/@formatjs+cli@6.7.2_@vue+compiler-core@3.5.17/node_modules/@formatjs/cli/bin/node_modules:/Users/rasikamaduranga/WS02/OpenChoreo/openchoreo/choreo-ui/common/temp/node_modules/.pnpm/@formatjs+cli@6.7.2_@vue+compiler-core@3.5.17/node_modules/@formatjs/cli/node_modules:/Users/rasikamaduranga/WS02/OpenChoreo/openchoreo/choreo-ui/common/temp/node_modules/.pnpm/@formatjs+cli@6.7.2_@vue+compiler-core@3.5.17/node_modules/@formatjs/node_modules:/Users/rasikamaduranga/WS02/OpenChoreo/openchoreo/choreo-ui/common/temp/node_modules/.pnpm/@formatjs+cli@6.7.2_@vue+compiler-core@3.5.17/node_modules:/Users/rasikamaduranga/WS02/OpenChoreo/openchoreo/choreo-ui/common/temp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rasikamaduranga/WS02/OpenChoreo/openchoreo/choreo-ui/common/temp/node_modules/.pnpm/@formatjs+cli@6.7.2_@vue+compiler-core@3.5.17/node_modules/@formatjs/cli/bin/node_modules:/Users/rasikamaduranga/WS02/OpenChoreo/openchoreo/choreo-ui/common/temp/node_modules/.pnpm/@formatjs+cli@6.7.2_@vue+compiler-core@3.5.17/node_modules/@formatjs/cli/node_modules:/Users/rasikamaduranga/WS02/OpenChoreo/openchoreo/choreo-ui/common/temp/node_modules/.pnpm/@formatjs+cli@6.7.2_@vue+compiler-core@3.5.17/node_modules/@formatjs/node_modules:/Users/rasikamaduranga/WS02/OpenChoreo/openchoreo/choreo-ui/common/temp/node_modules/.pnpm/@formatjs+cli@6.7.2_@vue+compiler-core@3.5.17/node_modules:/Users/rasikamaduranga/WS02/OpenChoreo/openchoreo/choreo-ui/common/temp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@formatjs/cli/bin/formatjs" "$@"
else
  exec node  "$basedir/../@formatjs/cli/bin/formatjs" "$@"
fi
